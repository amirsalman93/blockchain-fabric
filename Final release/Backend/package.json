{
    "name": "fabcar",
    "version": "1.0.0",
    "description": "FabCar application implemented in JavaScript",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "start": "./node_modules/nodemon/bin/nodemon.js src/app.js",
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "nyc mocha --recursive"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "express": "^4.17.1",
        "fabric-ca-client": "~1.4.0",
        "fabric-network": "~1.4.0",
        "mongodb": "^3.4.1",
        "socket.io": "^2.3.0"
    },
    "devDependencies": {
        "body-parser": "^1.18.3",
        "chai": "^4.2.0",
        "cors": "^2.8.5",
        "eslint": "^6.5.0",
        "handlebars": "^4.5.3",
        "mocha": "^5.2.0",
        "morgan": "^1.9.1",
        "nodemon": "^1.18.9",
        "nyc": "^14.1.1",
        "sinon": "^7.1.1",
        "sinon-chai": "^3.3.0",
        "tar": ">=5.0.10"
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "test/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
